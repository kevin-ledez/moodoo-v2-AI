{% extends 'base.html.twig' %}

{% block title %}NavigationLink{% endblock %}

{% block body %}
    <h1>NavigationLink</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ navigation_link.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ navigation_link.title }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ navigation_link.url }}</td>
            </tr>
            <tr>
                <th>Position</th>
                <td>{{ navigation_link.position }}</td>
            </tr>
            <tr>
                <th>IsVisible</th>
                <td>{{ navigation_link.isVisible ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ navigation_link.createdAt ? navigation_link.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ navigation_link.updatedAt ? navigation_link.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_navigation_link_index') }}">back to list</a>

    <a href="{{ path('app_navigation_link_edit', {'id': navigation_link.id}) }}">edit</a>

    {{ include('navigation_link/_delete_form.html.twig') }}
{% endblock %}
